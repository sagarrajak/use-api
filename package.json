{
  "name": "use-api",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix --ext ts,tsx  .",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "lodash": "^4.17.21",
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x",
    "react-redux": "^8.1.3"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/use-api.es.js",
      "require": "./dist/use-api.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/use-api.umd.js",
  "module": "./dist/use-api.es.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "typescript": "*",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3",
    "vite-plugin-linter": "^2.0.7",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn  eslint",
      "yarn  format"
    ],
    "*.{js,css,md,html,json}": "prettier --cache --write"
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "keywords": [],
  "author": "sagar rajak",
  "bugs": {
    "url": "https://github.com/sagarrajak/use-api/issues"
  },
  "homepage": "https://github.com/sagarrajak/use-api#readme",
  "license": "MIT"
}
